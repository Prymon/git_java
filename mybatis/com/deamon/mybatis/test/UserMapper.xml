<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deamon.mybatis.dao.UserDao">
	<select id="getUserById" resultType="com.deamon.mybatis.entity.User" parameterType="java.lang.String">
		select * from user where id = #{id}
	</select>
	
	<select id="getAllUser" resultType="com.deamon.mybatis.entity.User">
		select * from user
	</select>
	
</mapper>